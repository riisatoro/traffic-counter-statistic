{
    "name": "Chrome Traffic Counter",
    "description": "Count visited websites",
    "version": "0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "./extension/worker.js"
    },
    "action": {
        "default_popup": "./extension/main.html"
    },
    "permissions": [
        "activeTab", 
        "scripting", 
        "tabs", 
        "storage"
    ]
}
